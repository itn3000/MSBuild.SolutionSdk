<Project>

  <PropertyGroup>
    <MSBuildAllProjects>$(MSBuildAllProjects);$(MSBuildThisFileFullPath)</MSBuildAllProjects>
  </PropertyGroup>

  <Target Name="GetProjectMetadata" Returns="@(_ProjectMetadata)">

    <ItemGroup>
      <_ProjectMetadata Include="$(MSBuildProjectFullPath)">
        <UsingMicrosoftNETSdk>$(UsingMicrosoftNETSdk)</UsingMicrosoftNETSdk>
        <OutputPath>$(OutputPath)</OutputPath>
        <Configurations>$(Configurations)</Configurations>
        <Platforms>$(Platforms)</Platforms>
        <OriginalConfiguration>$(OriginalConfiguration)</OriginalConfiguration>
        <OriginalPlatform>$(OriginalConfiguration)</OriginalPlatform>
      </_ProjectMetadata>
    </ItemGroup>

  </Target>

</Project>